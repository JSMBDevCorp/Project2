<div class="card">
  <div class="card-content center-align">
      <img src="assets/images/logo-horizontal.png">
      <h4 class="white-text">ALL STOCKS</h4>
      
      <!--START STOCK EXAMPLE-->
      <ul class="list-group ">
        {{#each stock}}
          <li >
            <div class="stock valign-wrapper">
                <div>

                  <button class="delete" type="button" data-id="{{id}}">Delete</button>

                </div>
                <div>
                  <button class="update" type="button" data-id="{{id}}">Update</button>
                </div>

                <div class="stockpic">
                    <img class="center-align" src="{{imageURL}}" alt="stockLogo">
                </div>
                <div class="stocksymbol">
                    {{symbol}}
                </div>

                <div class="stockprice" style="color:{{getColor changes}}">
                 
                    {{price}}
                 

                <div class="stockprice ">
                    ${{price}}

                </div>
                {{!-- <div class="stockarrow ">
                    <img src="assets/images/arrow-up.png">
                </div> --}}
            </div>
          </li>
        {{/each}}
      </ul>
      <!--END STOCK EXAMPLE-->
      
      <form action="index.html">
          <div class="card-action center-align">
              <a href="/limit"><button type="button" class="btn">Filter</button></a>
              <a href="/api"><button type="button" class="btn">Add</button></a>
          </div>
      </form>
  </div>
</div>

